    "clean": "rm -rf dist",
    "test": "jest"
  },
  "author": "Dark-Light-20",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "ts-jest": "^29.4.6",
    "tsup": "^8.5.1",
    "typescript": "^5.9.2"
  },
  "publishConfig": {
    "access": "public"
  }
