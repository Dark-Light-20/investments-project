{
  "name": "@dark-light-20/invest-domain",
  "version": "1.0.2",
  "type": "module",
  "description": "This package contains the core domain logic for the investments project, including entities, value objects, and business rules.",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
